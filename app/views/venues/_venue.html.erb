<div class="row venue-partial border">
  <div class="col-xs-9">
    <div class="row">
      <%= venue.send(locals[:emphasis]) %>
    </div>
    <div class="row">
      <%= venue.name %>
      <%= venue.display_address %>
    </div>
  </div>
  <div class="col-xs-3">
    <%= link_to venue_path(venue) do %>
      <%= evil_icon 'ei-chevron-right' %>
    <% end %>
  </div>
</div>
