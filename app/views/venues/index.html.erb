          <ul>
            <li>Highest Rated</li>
            <li>Most Reviewed</li>
            <li>Closest to Me</li>
            <li>Open Latest</li>
          </ul>
         <p> - - - </p>
          <ul>
            <li>best wifi</li>
            <li>best coffee</li>
            <li>cheapest coffee</li>
          </ul>
          <p>- - - </p>
          <ul>
            <li>add a place</li>  
            <li><%= text_field_tag "query", "", placeholder: "Specific Place...?" %></li>
          </ul>
        </div>

        <div class="col-md-9">
          <% render @venues %>
        </div>
      </div>
    </div><!-- end .container -->
